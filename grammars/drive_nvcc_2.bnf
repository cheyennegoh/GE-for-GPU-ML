<block> ::= <op> 
          | if_gt("<arg>", "<arg>") + "\n" + <op>
          | <block> + "\n" + <block>

<op>    ::= <fp32>

<fp32>  ::= <num>("r[0]", "<arg>")
          | <num>("<ri>", "r[0]")
          | <fun>("r[0]")

<num>   ::= add | sub | mul | pdiv
<fun>   ::= sin | cos

<arg>   ::= <ri> | <xj> | <const>

<ri>    ::= r[<i>]
<xj>    ::= x[<j>]

<i>     ::= 0 | 1
<j>     ::= 0 | 1 | 2 | 3 | 4 | 5 | 6 | 7 | 8

<const> ::= 1 | 2 | 3 | 4 | 5 | 6 | 7 | 8 | 9